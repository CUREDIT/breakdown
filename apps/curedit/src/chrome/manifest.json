{
  "manifest_version": 2,
  "name": "Hg Cure",
  "version": "0.0.1",
  "description": "Automate your browsing",
  "background": {
    "scripts": ["background.curedit.js", "runtime.curedit.js"]
  },
  "browser_action": {
    "default_popup": "./curemedit/index.html"
  },
  "permissions": ["tabs", "webNavigation", "storage"],
  "options_page": "./curedit/index.html",
  "minimum_chrome_version": "61"
}
