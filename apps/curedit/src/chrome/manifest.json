{
  "manifest_version": 2,
  "name": "nG auto",
  "version": "0.0.1",
  "description": "Automate your browser",
  "background": {
    "scripts": [
      "./curedit/background-es2015.js",
      "./curedit/runtime-es2015.js"
    ]
  },
  "browser_action": {
    "default_popup": "./curemedit/index.html"
  },
  "permissions": [
    "tabs",
    "webNavigation"
  ]
}
